{
  "name": "qindel-clean_code-intro",
  "version": "22.6.0",
  "description": "Curso de introducción al Código Limpio para Qindel",
  "author": {
    "name": "Alberto Basalo",
    "email": "albertobasalo@hotmail.com",
    "url": "https://twitter.com/albertobasalo"
  },
  "scripts": {
    "start": "node ./dist/main.js",
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node ./src/main.ts",
    "ts-node": "ts-node",
    "format": "prettier --write \"./**/*.{ts,json}\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "update": "ncu -u",
    "prerelease": "standard-version ",
    "release": "git push --follow-tags origin main"
  },
  "devDependencies": {
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-check-updates": "^13.1.1",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "ts-node": "^10.8.1"
  },
  "dependencies": {
    "typescript": "^4.7.3"
  }
}